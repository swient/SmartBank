<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="10.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="io.github.swient.smartbank.controller.AdminController">
    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>
    <HBox spacing="10">
        <Label text="銀行:"/>
        <Label fx:id="bankLabel"/>
    </HBox>
    <TreeView fx:id="userTreeView" prefHeight="400.0" prefWidth="350.0" />
    <Button text="登出" onAction="#onLogoutClick" prefWidth="100.0" />
    <Label fx:id="logoutMsg" textFill="red"/>
</VBox>
